<?xml version="1.0" encoding="UTF-8"?>
<web-app version="5.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd">
    <servlet>
        <servlet-name>OnlineShop</servlet-name>
        <servlet-class>shop.OnlineShop</servlet-class>
        <init-param>
            <param-name>Laptop</param-name>
            <param-value>50000</param-value>
        </init-param>
        <init-param>
            <param-name>ac</param-name>
            <param-value>5000</param-value>
        </init-param>
        <init-param>
            <param-name>mobile</param-name>
            <param-value>7000</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>OnlineShop</servlet-name>
        <url-pattern>/OnlineShop</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>shopping.html</welcome-file>
    </welcome-file-list>
</web-app>
